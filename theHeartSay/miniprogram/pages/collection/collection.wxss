.container{
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
}
.none{
  line-height: 1000rpx;
  height: 1000rpx;
}

.collectionCard{
  width: 640rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
}
.picBox{
  position: relative;
  /* border: 1px solid #000; */
  border-radius: 20rpx;
  box-shadow: 0px 2px 5px 5px #f1f1f1;
  margin: 20rpx;
  width: 280rpx;
  height: 260rpx;
}
.collectionCard-pic{
  border-radius: 20rpx;
  width: 280rpx;
  height: 200rpx;
}
.collectionCard-icon{
  position: absolute;
  bottom: 13rpx;
  right: 20rpx;
  width: 30rpx;
  height: 30rpx;
}
.title{
  text-align: center;
  vertical-align: middle;
  color: darkgrey;
  vertical-align: middle;
  font-size: 80%;
  line-height: 50rpx;
  height: 50rpx;
}